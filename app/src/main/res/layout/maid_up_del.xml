<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"

    android:layout_height="match_parent"
    tools:context=".MaidHomePanel">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="sans-serif-thin"
            android:padding="8dp"
            android:textColor="@color/grey1"
            android:textSize="40sp"
            android:layout_marginTop="10dp"
            android:layout_gravity="center"
            android:text="Update and Delete" />

        <TextView
            android:id="@+id/textView16"
            android:layout_width="297dp"
            android:layout_height="45dp"
            android:textColor="@color/colorPrimary"
            android:textSize="26sp"
            android:textAlignment="center"
            android:layout_gravity="center"
            android:layout_marginTop="18dp"
            android:ems="10"
            android:inputType="textPersonName"
            />

        <Button
            android:id="@+id/button2"
            android:layout_width="180dp"
            android:layout_height="50dp"
            android:layout_gravity="center"
            android:background="@color/colorPrimary"
            android:textColor="@color/white"
            android:layout_marginTop="15dp"
            android:text="Search Details" />

        <EditText
            android:id="@+id/editText2"
            android:layout_width="297dp"
            android:layout_height="45dp"
            android:textColor="@color/grey1"
            android:textColorHint="@color/colorPrimary"
            android:layout_gravity="center"
            android:layout_marginTop="30dp"
            android:ems="10"
            android:hint="new username"
            android:inputType="textPersonName" />

        <EditText
            android:id="@+id/editText3"
            android:layout_width="297dp"
            android:layout_height="45dp"
            android:textColor="@color/grey1"
            android:textColorHint="@color/colorPrimary"
            android:layout_gravity="center"
            android:layout_marginTop="30dp"
            android:ems="10"
            android:hint="new password"
            android:inputType="textPersonName" />

        <EditText
            android:id="@+id/editText4"
            android:layout_width="297dp"
            android:layout_height="45dp"
            android:textColor="@color/grey1"
            android:textColorHint="@color/colorPrimary"
            android:layout_gravity="center"
            android:layout_marginTop="30dp"
            android:ems="10"
            android:hint="age"
            android:inputType="textPersonName" />


        <EditText
            android:id="@+id/editText12"
            android:layout_width="297dp"
            android:layout_height="45dp"
            android:textColor="@color/grey1"
            android:textColorHint="@color/colorPrimary"
            android:layout_gravity="center"
            android:layout_marginTop="30dp"
            android:ems="10"
            android:hint="new contact no."
            android:inputType="textPersonName" />

        <EditText
            android:id="@+id/editText9"
            android:layout_width="297dp"
            android:layout_height="45dp"
            android:textColor="@color/grey1"
            android:textColorHint="@color/colorPrimary"
            android:layout_gravity="center"
            android:layout_marginTop="30dp"
            android:ems="10"
            android:hint="new gender"
            android:inputType="textPersonName" />
        <TextView
            android:id="@+id/textView4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:layout_marginBottom="20dp"

            android:text="Change Your Location Details"
            android:textAlignment="center"
            android:textSize="20dp"
            android:textColor="@color/grey1"
            android:textStyle="bold"
            android:gravity="center_horizontal" />

        <EditText
            android:id="@+id/editText10"
            android:layout_width="297dp"
            android:layout_height="45dp"
            android:textColor="@color/grey1"
            android:textColorHint="@color/colorPrimary"
            android:layout_gravity="center"
            android:layout_marginTop="20dp"
            android:ems="10"
            android:hint="new state"
            android:inputType="textPersonName" />

        <EditText
            android:id="@+id/editText13"
            android:layout_width="297dp"
            android:layout_height="45dp"
            android:textColor="@color/grey1"
            android:textColorHint="@color/colorPrimary"
            android:layout_gravity="center"
            android:layout_marginTop="30dp"
            android:ems="10"
            android:hint="new city"
            android:inputType="textPersonName" />

        <EditText
            android:id="@+id/editText14"
            android:layout_width="297dp"
            android:layout_height="45dp"
            android:textColor="@color/grey1"
            android:textColorHint="@color/colorPrimary"
            android:layout_gravity="center"
            android:layout_marginTop="30dp"
            android:ems="10"
            android:hint="new locality"
            android:inputType="textPersonName" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:layout_marginLeft="30dp"
            android:layout_marginTop="30dp"
            android:layout_marginRight="30dp"
            android:textSize="20dp"
            android:text="Change Your Work Specification Details"
            android:textAlignment="center"
            android:textColor="@color/grey1"
            android:textStyle="bold"
            android:gravity="center_horizontal" />

        <CheckBox
            android:id="@+id/checkBox7"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="30dp"
            android:layout_marginRight="30dp"
            android:text="Moping" />

        <CheckBox
            android:id="@+id/checkBox8"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="20dp"
            android:layout_marginRight="30dp"
            android:text="Dish washing" />

        <CheckBox
            android:id="@+id/checkBox9"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="20dp"
            android:layout_marginRight="30dp"
            android:text="Brooming" />

        <CheckBox
            android:id="@+id/checkBox10"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="20dp"
            android:layout_marginRight="30dp"
            android:text="Cloths washing" />

        <CheckBox
            android:id="@+id/checkBox11"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="20dp"
            android:layout_marginRight="30dp"
            android:text="Cooking" />

        <CheckBox
            android:id="@+id/checkBox12"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="20dp"
            android:layout_marginRight="30dp"
            android:text="Dusting" />







            <TextView
                android:id="@+id/textView99"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp"
                android:layout_marginBottom="20dp"

                android:text="Feedback From User"
                android:textAlignment="center"
                android:textSize="20dp"
                android:textColor="@color/grey1"
                android:textStyle="bold"
                android:gravity="center_horizontal"  />


        <TextView
            android:id="@+id/textView10"
            android:layout_width="374dp"
            android:layout_height="135dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="10dp"
            android:layout_marginRight="20dp"
            android:layout_weight="1"
            android:fontFamily="serif-monospace"
            android:gravity="center_horizontal"

            android:textAlignment="center"
            android:textColor="@color/colorPrimary"
            android:textSize="18sp"
            android:textStyle="bold" />



















        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="25dp"
            android:orientation="horizontal">

            <Button
                android:id="@+id/button5"
                android:layout_width="135dp"
                android:layout_height="50dp"
                android:background="@color/colorPrimary"
                android:textColor="@color/white"

                android:layout_marginLeft="30dp"
                android:layout_gravity="center_horizontal"
                android:text="Update" />

            <Button
                android:id="@+id/button6"
                android:layout_width="135dp"
                android:layout_marginLeft="30dp"
                android:layout_gravity="center_horizontal"
                android:layout_height="50dp"
                android:background="@color/colorPrimary"
                android:textColor="@color/white"

                android:text="Delete" />
        </LinearLayout>
        </LinearLayout>

    </ScrollView>
</androidx.appcompat.widget.LinearLayoutCompat>