<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
   android:orientation="vertical"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MaidFeatures">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".adminMain">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"

            android:orientation="vertical"
            tools:context=".UserUpdateDelete">

            <ScrollView
                android:layout_width="match_parent"
                android:layout_height="match_parent">


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/textView11"

                        android:layout_width="319dp"
                        android:layout_height="89dp"
                        android:layout_gravity="center"
                        android:layout_marginTop="20dp"
                        android:ems="10"

                        android:inputType="textPersonName"
                        android:textAlignment="center"
                        android:textColor="@color/colorPrimary"
                        android:textSize="18sp"
                        android:textStyle="bold" />


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="90dp"
                        android:layout_marginLeft="20dp"
                        android:layout_marginRight="20dp"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/textView20"
                            android:layout_width="157dp"
                            android:layout_height="75dp"
                            android:layout_weight="1"
                            android:textSize="20sp"
                            android:textColor="@color/grey1"
                            android:text="Username :"
                            android:textStyle="bold"
                            />

                        <TextView
                            android:id="@+id/textView3"
                            android:layout_width="294dp"
                            android:layout_height="80dp"
                            android:layout_weight="1"
                            android:textColor="@color/colorPrimary"
                            android:textSize="22sp" />
                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="90dp"

                        android:layout_marginLeft="20dp"
                        android:layout_marginRight="20dp"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/textView21"
                            android:layout_width="157dp"
                            android:layout_height="75dp"
                            android:textSize="20sp"
                            android:textColor="@color/grey1"
                            android:layout_weight="1"
                            android:text="Age :"
                            android:textStyle="bold"
                            />

                        <TextView
                            android:id="@+id/textView4"
                            android:layout_width="294dp"
                            android:layout_height="80dp"
                            android:textColor="@color/colorPrimary"
                            android:textSize="22sp"
                            android:layout_weight="1" />


                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="90dp"

                        android:layout_marginLeft="20dp"
                        android:layout_marginRight="20dp"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/textView23"
                            android:layout_width="157dp"
                            android:layout_height="75dp"
                            android:textSize="20sp"
                            android:textColor="@color/grey1"
                            android:layout_weight="1"
                            android:textStyle="bold"
                            android:text="Contact Number :" />

                        <TextView
                            android:id="@+id/textView5"
                            android:layout_width="294dp"
                            android:layout_height="80dp"
                            android:textColor="@color/colorPrimary"
                            android:textSize="22sp"
                            android:layout_weight="1" />
                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="90dp"

                        android:layout_marginLeft="20dp"
                        android:layout_marginRight="20dp"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/textView231"
                            android:layout_width="157dp"
                            android:layout_height="75dp"
                            android:layout_weight="1"
                            android:text="Gender :"
                            android:textSize="20sp"
                            android:textColor="@color/grey1"
                            android:textStyle="bold"
                            />

                        <TextView
                            android:id="@+id/textView6"
                            android:layout_width="294dp"
                            android:textColor="@color/colorPrimary"
                            android:textSize="22sp"
                            android:layout_height="80dp"
                            android:layout_weight="1" />
                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="90dp"

                        android:layout_marginLeft="20dp"
                        android:layout_marginRight="20dp"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/textView211"
                            android:layout_width="157dp"
                            android:layout_height="75dp"
                            android:layout_weight="1"
                            android:textSize="20sp"
                            android:textColor="@color/grey1"
                            android:text="State :"
                            android:textStyle="bold"/>

                        <TextView
                            android:id="@+id/textView7"
                            android:layout_width="294dp"

                            android:layout_height="80dp"
                            android:textColor="@color/colorPrimary"
                            android:textSize="22sp"
                            android:layout_weight="1" />
                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="90dp"

                        android:layout_marginLeft="20dp"
                        android:layout_marginRight="20dp"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/textView241"
                            android:textStyle="bold"
                            android:layout_width="157dp"
                            android:textSize="20sp"
                            android:textColor="@color/grey1"
                            android:layout_height="75dp"
                            android:layout_weight="1"
                            android:text="City :" />

                        <TextView
                            android:id="@+id/textView8"
                            android:layout_width="294dp"
                            android:textColor="@color/colorPrimary"
                            android:textSize="22sp"
                            android:layout_height="80dp"
                            android:layout_weight="1" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="90dp"

                        android:layout_marginLeft="20dp"
                        android:layout_marginRight="20dp"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/textView281"
                            android:layout_width="157dp"
                            android:layout_height="75dp"
                            android:textSize="20sp"
                            android:textColor="@color/grey1"
                            android:layout_weight="1"
                            android:text="Locality :"
                            android:textStyle="bold"/>

                        <TextView
                            android:id="@+id/textView9"
                            android:layout_width="294dp"
                            android:textColor="@color/colorPrimary"
                            android:textSize="22sp"
                            android:layout_height="80dp"
                            android:layout_weight="1" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="89dp"
                        android:orientation="horizontal">

                        <Button
                            android:id="@+id/button9"
                            android:layout_width="match_parent"
                            android:background="@color/colorPrimary"
                            android:textColor="@color/white"
                            android:layout_weight="1"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="20dp"
                            android:layout_marginTop="30dp"
                            android:layout_marginRight="20dp"

                            android:text="CALL MAID"
                             />

                        <Button
                            android:id="@+id/button10"
                            android:layout_width="match_parent"
                            android:background="@color/colorPrimary"
                            android:textColor="@color/white"
                            android:layout_weight="1"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="20dp"
                            android:layout_marginTop="30dp"
                            android:layout_marginRight="20dp"
                            android:text="GIVE FEEDBACK"
                            />
                    </LinearLayout>




                        <Button
                            android:id="@+id/button11"
                            android:layout_width="match_parent"
                            android:background="@color/colorPrimary"
                            android:textColor="@color/white"
                            android:layout_weight="1"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="110dp"
                            android:layout_marginTop="20dp"
                            android:layout_marginRight="110dp"
                            android:text="SEND COMPLAINS "
                             />

                        <Button
                            android:id="@+id/button12"
                            android:layout_width="match_parent"
                            android:background="@color/colorPrimary"
                            android:textColor="@color/white"
                            android:layout_weight="1"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="20dp"
                            android:layout_marginTop="30dp"
                            android:layout_marginRight="20dp"
                            android:text="LOGOUT"
                             />



                </LinearLayout>

            </ScrollView>

        </LinearLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.appcompat.widget.LinearLayoutCompat>